<% form_for :feature, url: "#", html: {class: "form-horizontal"} do |f| %>
	<div class="form-group">
		<%= f.label "Name" %>
		<%= f.text_field "name" %>
	</div>
	<div class="form_actions">
		<%= link_to "Back", "#", class: 'btn back'%>
		<%= f.submit "Create Feature", class: "btn btn-primary button" %>
	</div>
<% end %>
