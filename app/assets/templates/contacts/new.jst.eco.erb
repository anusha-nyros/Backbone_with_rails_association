<h1>New Contact </h1>
<% form_for :contact, url: "#", html: {class: "form-horizontal"} do |f| %>
	<div class="form-group">
		<%= f.label "Name" %>
		<%= f.text_field "name" %>
	</div>
	<div class="form-group">
		<%= f.label "Email" %></label>
 		<%= f.text_field "email" %>
	</div>
	<div class="form-group">
		<%= f.label "Phone" %>
		<%= f.text_field "phone" %>
        </div>
	<div class="form-group" id="error"></div>
	<div class="form_actions">
		<%= link_to "Back", "#/listings", class: 'btn back'%>
		<%= f.submit "Create contact", class: "btn btn-primary button" %>
	</div>
<% end %>
